{% extends 'base.html.twig' %}

{% block title %}Hello ImportExcelActiveSubGroupingController!{% endblock %}

{% block body %}

<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th scope="col">Active Substance (High Level)</th>
            <th scope="col">Active Substance (Low Level)</th>
        </tr>
    </thead>
    <tbody>
        {% for ActSubGrp in tabActSubGrp %}
            <tr>
                <td>{{ActSubGrp.AS_HL}}</td>
                <td>{{ActSubGrp.AS_LL}}</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

{% endblock %}
